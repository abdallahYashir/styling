<p id="notice"><%= notice %></p>

<h1>Schedule Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedule_types.each do |schedule_type| %>
      <tr>
        <td><%= schedule_type.name %></td>
        <td><%= link_to 'Show', schedule_type %></td>
        <td><%= link_to 'Edit', edit_schedule_type_path(schedule_type) %></td>
        <td><%= link_to 'Destroy', schedule_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule Type', new_schedule_type_path %>
