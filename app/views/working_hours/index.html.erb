<p id="notice"><%= notice %></p>

<h1>Working Hours</h1>

<table>
  <thead>
    <tr>
      <th>Days</th>
      <th>Active</th>
      <th>Time start</th>
      <th>Time end</th>
      <th>Business</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @working_hours.each do |working_hour| %>
      <tr>
        <td><%= working_hour.days %></td>
        <td><%= working_hour.active %></td>
        <td><%= working_hour.time_start %></td>
        <td><%= working_hour.time_end %></td>
        <td><%= working_hour.business %></td>
        <td><%= link_to 'Show', working_hour %></td>
        <td><%= link_to 'Edit', edit_working_hour_path(working_hour) %></td>
        <td><%= link_to 'Destroy', working_hour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Working Hour', new_working_hour_path %>
